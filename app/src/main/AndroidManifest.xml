<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.homework.ahhstatistic">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".investor.deleted.DeletedInvestorDetailActivity"
            android:screenOrientation="portrait"
            android:parentActivityName=".investor.deleted.DeletedInvestorCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".investor.deleted.DeletedInvestorCategoryActivity"
            android:parentActivityName=".investor.InvestorCategoryActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".Income.MyIncomeDetailActivity"
            android:exported="false"
            android:parentActivityName=".investor.InvestorCategoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Income.MyIncomeCategoryActivity"
            android:exported="false"
            android:parentActivityName=".Income.MyIncomeHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Income.MyIncomeHomeActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profit.MonthlyProfitDetailActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.ProfitDialog" />
        <activity
            android:name=".profit.MonthlyProfitCategoryActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".investor.UpdateInvestorActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.LogReg" />
        <activity
            android:name=".investor.detail.InvestorDetailActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:parentActivityName=".investor.InvestorCategoryActivity"
            android:theme="@style/AppTheme.Yellow" />
        <activity
            android:name=".investor.AddInvestorActivity"
            android:exported="false"
            android:parentActivityName=".investor.InvestorCategoryActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.LogReg" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.LogReg" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.LogReg" />
        <activity
            android:name=".investor.InvestorCategoryActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".calculator.DailyProfitCalculator"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>